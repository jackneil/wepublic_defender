# WePublicDefender API Keys
# Copy this file to .env and fill in your actual API keys

# OpenAI API Key (for GPT-5, GPT-4o)
OPENAI_API_KEY=your_openai_api_key_here

# xAI API Key (for Grok 4, Grok 4 Fast)
XAI_API_KEY=your_xai_api_key_here

# Optional: Groq API Key (for alternative models)
# GROQ_API_KEY=your_groq_api_key_here

# CourtListener API Token (for case law research and citation verification)
# Get your free token at: https://www.courtlistener.com/api/
COURTLISTENER_TOKEN=your_courtlistener_token_here

# CourtListener User Agent (required by API - use format: AppName/Version (+contact))
COURTLISTENER_USER_AGENT=WePublicDefender/0.1 (+your_email@example.com)
